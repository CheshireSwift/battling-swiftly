    "react-rough": "^2.0.0",
    "react-test-renderer": "^16.12.0",
    "roughjs": "^3.1.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/jsdom": "^12.2.3",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
